<!-- LISTAGEM -->
    <h3 class="mt-5 titulo">Usuário Cadastrados</h3>

    <!--DIVISÓRIA--->
    <div class="container">
        <hr class="text-center"></hr> 
    </div>   
    
    <table class="table table-hover table-responsive-sm text-center">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Nome</th>
          <th scope="col">Sobrenome</th>
          <th scope="col">Data de Nascimento</th>
          <th scope="col">Gênero</th>
          <th scope="col">Email</th>
          <th scope="col">Telefone</th>
          <th scope="col">Ativo</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
          {{#each posts}}
        <tr>
          <th scope="row" id="{{dataValues.id}}">{{dataValues.id}}</th>
          <td>{{dataValues.nome}}</td>
          <td>{{dataValues.sobrenome}}</td>
          <td>{{dataValues.data_nascimento}}</td>
          <td>{{dataValues.genero}}</td>
          <td>{{dataValues.email}}</td>
          <td>{{dataValues.telefone}}</td>
          <td>{{dataValues.ativo}}</td>
          <td><button class="btn btn-danger btn-sm"></i>Remover</button></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
       